<Window x:Class="TavernSkeep.SkeepHub"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        xmlns:local="clr-namespace:TavernSkeep"
        mc:Ignorable="d"
        Title="SkeepHub" Height="450" Width="800">
    <Grid>
        <Grid x:Name="content" Visibility="Collapsed">
            <Button Width="100px" Height="20px" Content="uwu" HorizontalAlignment="Center" VerticalAlignment="Center"></Button>
        </Grid>
        <Image Name="loadbg" gif:ImageBehavior.AnimatedSource="/TavernSkeep;component/medieval-inn.gif" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Stretch="Fill"/>
        <Image Name="loaddots" gif:ImageBehavior.AnimatedSource="/TavernSkeep;component/loading-dots-white2.gif" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Window>
